{
  "name": "consolji",
  "version": "0.0.3",
  "description": "Elegant Console Wrapper",
  "keywords": [
    "console",
    "logger",
    "reporter",
    "elegant",
    "cli",
    "universal",
    "unified",
    "prompt",
    "clack",
    "format",
    "error",
    "stacktrace"
  ],
  "repository": "unjs/consola",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "node": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./lib/index.cjs"
      },
      "default": {
        "types": "./dist/index.browser.d.ts",
        "import": "./dist/index.browser.mjs"
      }
    },
    "./browser": {
      "types": "./dist/index.browser.d.ts",
      "import": "./dist/index.browser.mjs"
    },
    "./basic": {
      "node": {
        "types": "./dist/index.basic.d.ts",
        "import": "./dist/index.basic.mjs",
        "require": "./dist/index.basic.cjs"
      },
      "default": {
        "types": "./dist/index.browser.d.ts",
        "import": "./dist/index.browser.mjs"
      }
    },
    "./core": {
      "types": "./dist/index.core.d.ts",
      "import": "./dist/index.core.mjs",
      "require": "./dist/index.core.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/index.browser.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "lib",
    "*.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "pnpm test && pnpm build && changelogen --release --push && npm publish",
    "test": "pnpm lint && pnpm vitest run --coverage"
  },
  "devDependencies": {
    "@nyxb/colorette": "^2.0.23",
    "@nyxb/eslint-config": "^0.0.52",
    "@nyxb/picocolors": "^1.0.1",
    "@tyck/core": "^0.0.1",
    "@types/gradient-string": "^1.1.2",
    "@types/node": "^18.16.3",
    "@types/sentencer": "^0.2.0",
    "@vitest/coverage-c8": "^0.30.1",
    "changelogen": "^0.5.3",
    "defu": "^6.1.2",
    "eslint": "^8.39.0",
    "gradient-string": "^2.0.2",
    "is-unicode-supported": "^1.3.0",
    "jiti": "^1.18.2",
    "lodash": "^4.17.21",
    "printj": "^1.3.1",
    "sentencer": "^0.2.1",
    "sisteransi": "^1.0.5",
    "std-env": "^3.3.2",
    "string-width": "^5.1.2",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vitest": "^0.30.1",
    "winston": "^3.8.2"
  },
  "packageManager": "pnpm@8.4.0"
}
