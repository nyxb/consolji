{
  "extends": [
    "@nyxb"
  ],
  "rules": {
    "@next/next/no-html-link-for-pages": 0,
    "no-console": 0,
    "no-undef": 0,
    "unicorn/prefer-module": 0,
    "unicorn/prefer-top-level-await": 0,
    "unicorn/no-null": 0,
    "@typescript-eslint/no-unused-vars": 0
  },
  "settings": {
    "react": {
      "version": "18.0"
    }
  }
}
